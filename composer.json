    "type": "library",
    "require": {
        "php": "^7.0",
        "laravel/framework": "^5.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Daiki",
            "email": "nojiri1098@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nojiri1098\\Annotate\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nojiri1098\\Annotate\\ServiceProvider"
            ]
        }
    }
}
